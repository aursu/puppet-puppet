webserver: {
    # Log webserver access to a specific file.
    access-log-config: <%= @server_confdir %>/request-logging.xml
    # Require a valid certificate from the client.
    client-auth: <%= @client_auth %>
    # Listen for HTTPS traffic on all available hostnames.
    ssl-host: 0.0.0.0
    # Listen for HTTPS traffic on port 8140.
    ssl-port: 8140
    ssl-cert: <%= @ssl_cert %>
    ssl-key: <%= @ssl_key %>
    ssl-ca-cert: <%= @ssl_ca_cert %>
    <% if @ssl_cert_chain -%>
    ssl-cert-chain: <%= @ssl_cert_chain %>
    <% end -%>
    ssl-crl-path: <%= @ssl_crl_path %>
}
